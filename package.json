{
  "name": "mqtt-explorer",
  "version": "1.0.0",
  "description": "Professional MQTT Client & Explorer for Windows",
  "main": "dist/main/main/main.js",
  "author": "Huseyin2603",
  "license": "MIT",
  "scripts": {
    "dev:renderer": "vite",
    "dev:main": "tsc -p tsconfig.main.json && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "dev": "concurrently \"npm run dev:renderer\" \"wait-on http://localhost:5173 && npm run dev:main\"",
    "build:renderer": "vite build",
    "build:main": "tsc -p tsconfig.main.json",
    "build": "npm run build:renderer && npm run build:main",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.0.0",
    "allotment": "^1.0.0",
    "clsx": "^2.0.0",
    "date-fns": "^4.0.0",
    "diff": "^7.0.0",
    "fast-xml-parser": "^5.0.0",
    "file-saver": "^2.0.0",
    "immer": "^10.0.0",
    "jsonpath-plus": "^10.0.0",
    "lucide-react": "^0.400.0",
    "mqtt": "^5.0.0",
    "papaparse": "^5.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.0.0",
    "react-virtuoso": "^4.0.0",
    "tailwind-merge": "^2.0.0",
    "uuid": "^11.0.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@types/diff": "^6.0.0",
    "@types/file-saver": "^2.0.0",
    "@types/papaparse": "^5.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^9.0.0",
    "cross-env": "^10.1.0",
    "electron": "^33.0.0",
    "electron-builder": "^25.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0",
    "wait-on": "^8.0.0"
  }
}
